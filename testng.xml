<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DsAlgo_TestNgSuite" parallel="tests" thread-count="3">
  <listeners>
  	<listener class-name="listeners.RetryListener"/>
  	<listener class-name="listeners.ItestListener"/>
  	
   	
  </listeners>
  <test name="ChromeTest" >
	  <parameter name="browser" value="chrome"/>
      <classes>
           <class name="testCases.StackTests">
           <methods>
                    <include name="testStack" />
         </methods>
   </class>
       <class name="testCases.QueueTests">
         <methods>
         <include name="testQueue"/>
         </methods>
       </class>
     </classes>
    </test>
    <test name="EdgeTest" >
	  <parameter name="browser" value="edge"/>
      <classes>
           <class name="testCases.StackTests">
           <methods>
                    <include name="testStack" />
         </methods>
      </class>
       <class name="testCases.QueueTests">
         <methods>
         <include name="testQueue"/>
         </methods>
       </class>
     </classes>
    </test>
</suite> <!-- Suite -->